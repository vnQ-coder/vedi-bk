{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint \"src/**/*.{js,ts}\" ",
    "lintAutoFix": "./node_modules/.bin/eslint \"src/**/*.{js,ts}\"  --quiet  --fix; ",
    "dev": "nodemon --config nodemon.json src/app.ts",
    "build": "rm -rf dist && tsc",
    "start": "npm run lint; npm run build; ENV=development node dist/app.js",
    "db-fresh-run": "tsc; npm run dbRollback; npm run dbMigrateLatest; npm run dbSeed; npm start",
    "dbMigrateLatest": "cd dist/; ENV=development knex migrate:latest",
    "dbRollback": "cd dist/; ENV=development knex migrate:rollback",
    "dbReset": "npm run dbRollback; npm run dbMigrateLatest",
    "dbSeed": "cd dist/; ENV=development knex seed:run",
    "compileForProd": "./node_modules/@babel/cli/bin/babel.js src --out-dir  dist --presets minify ",
    "babel": "./node_modules/@babel/cli/bin/babel.js src --out-dir  dist ",
    "babelWithSourceMap": "./node_modules/@babel/cli/bin/babel.js src --out-dir  dist --source-maps inline ",
    "package": "npm run lint; npm run babel"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/connect-redis": "0.0.16",
    "@types/express-session": "^1.17.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.1.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "core-util-is": "^1.0.2",
    "country-state-city": "^2.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "generate-password": "^1.6.0",
    "http-status-codes": "^1.4.0",
    "knex": "^0.21.15",
    "mailgun-js": "^0.22.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.1",
    "postgres": "^1.0.2",
    "redis": "^3.1.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/mailgun-js": "^0.22.8",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^2.0.7",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.6"
  }
}
